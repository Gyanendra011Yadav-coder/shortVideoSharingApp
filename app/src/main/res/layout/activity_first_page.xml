<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />


    <VideoView
        android:id="@+id/videoView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="-54dp"
        tools:layout_editor_absoluteY="-16dp" />

    <ProgressBar
        android:id="@+id/videoProgressBar"
        android:layout_width="50dp"
        android:layout_height="50dp"
        tools:layout_editor_absoluteX="181dp"
        tools:layout_editor_absoluteY="341dp" />

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        tools:layout_editor_absoluteY="624dp">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="end"
            android:gravity="end"
            android:orientation="vertical">

        </LinearLayout>

    </LinearLayout>

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:src="@drawable/ic_videomake"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="53dp"
        android:layout_height="40dp"
        android:layout_gravity="end"
        android:padding="6dp"
        android:src="@drawable/ic_home"
        tools:layout_editor_absoluteX="9dp"
        tools:layout_editor_absoluteY="682dp" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="40dp"
        android:layout_height="33dp"
        android:src="@drawable/ic_search"
        tools:layout_editor_absoluteX="80dp"
        tools:layout_editor_absoluteY="689dp" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="53dp"
        android:layout_height="34dp"
        android:src="@drawable/ic_person"
        tools:layout_editor_absoluteX="342dp"
        tools:layout_editor_absoluteY="689dp" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="53dp"
        android:layout_height="34dp"
        android:src="@drawable/ic_notifications"
        tools:layout_editor_absoluteX="238dp"
        tools:layout_editor_absoluteY="689dp" />

    <ImageButton
        android:id="@+id/favorites"
        android:layout_width="40dp"
        android:layout_height="33dp"
        android:layout_gravity="end"
        android:padding="2dp"
        android:src="@drawable/ic_favorite"
        tools:layout_editor_absoluteX="357dp"
        tools:layout_editor_absoluteY="384dp" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="40dp"
        android:layout_height="34dp"
        android:layout_gravity="end"
        android:padding="2dp"
        android:src="@drawable/ic_comment"
        tools:layout_editor_absoluteX="357dp"
        tools:layout_editor_absoluteY="459dp" />

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="53dp"
        android:layout_height="34dp"
        android:layout_gravity="end"
        android:padding="2dp"
        android:src="@drawable/ic_baseline_add"
        tools:layout_editor_absoluteX="342dp"
        tools:layout_editor_absoluteY="526dp" />

    <ImageButton
        android:id="@+id/imageButton7"
        android:layout_width="53dp"
        android:layout_height="34dp"
        android:layout_gravity="end"
        android:padding="6dp"
        android:src="@drawable/ic_share"
        tools:layout_editor_absoluteX="342dp"
        tools:layout_editor_absoluteY="301dp" />

    <ImageButton
        android:id="@+id/imageButton8"
        android:layout_width="53dp"
        android:layout_height="34dp"
        android:layout_gravity="end"
        android:padding="4dp"
        android:src="@drawable/ic_add_circle"
        tools:layout_editor_absoluteX="152dp"
        tools:layout_editor_absoluteY="689dp" />

</androidx.constraintlayout.widget.ConstraintLayout>